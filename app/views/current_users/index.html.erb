<p id="notice"><%= notice %></p>

<h1>Listing Current Users</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @current_users.each do |current_user| %>
      <tr>
        <td><%= current_user.title %></td>
        <td><%= current_user.description %></td>
        <td><%= current_user.address %></td>
        <td><%= current_user.latitude %></td>
        <td><%= current_user.longitude %></td>
        <td><%= link_to 'Show', current_user %></td>
        <td><%= link_to 'Edit', edit_current_user_path(current_user) %></td>
        <td><%= link_to 'Destroy', current_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Current user', new_current_user_path %>
